---
layout: null
---
<?xml version="1.0" encoding="UTF-8" ?>
<articles>
    {% for post in site.posts %}
    <article>
        <title>{{ post.title }}</title>
        <url>{{ site.url }}{{ post.url }}</url>
        <tag>{{ post.tags }}</tag>
        <date>{{ post.date | date_to_utc | date: '%Y-%m-%d'}}</date>
    </article>
    {% endfor %}
</articles>
